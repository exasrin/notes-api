{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-console": "off",
        "camelcase": "off",
        "no-extraneous-dependencies": "off",
        "max-len":"off"
    }
}
